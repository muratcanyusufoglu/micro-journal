{
  "expo": {
    "name": "OneLine",
    "slug": "OneLine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dailyicon.png",
    "scheme": "oneline",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/dailysplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#F7F6F3"
    },
    "ios": {
      "buildNumber": "2",
      "bundleIdentifier": "com.oneline.dailynotes",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.oneline.dailynotes"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/dailysplash.png",
          "resizeMode": "cover",
          "imageWidth": 706,
          "backgroundColor": "#F7F6F3",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
